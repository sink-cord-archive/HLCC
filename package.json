{
  "name": "hlcc",
  "version": "1.4.0",
  "description": "The client mod agnostic tool to build Discord snippets",
  "main": "dist/transform.js",
  "module": "dist/transform.js",
  "bin": "./dist/index.js",
  "scripts": {
    "build": "hegel && flow-remove-types src/ --out-dir dist/ --all",
    "test": "\"$npm_execpath\" run build && node dist/index.js tests/snippet.js tests/compiled.js --nominify",
    "prepublish": "rm -rf dist && \"$npm_execpath\" run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yellowsink/HLCC.git"
  },
  "author": "Yellowsink",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/yellowsink/HLCC/issues"
  },
  "homepage": "https://github.com/yellowsink/HLCC#readme",
  "dependencies": {
    "@swc/core": "^1.2.135"
  },
  "devDependencies": {
    "@hegel/cli": "^0.0.45",
    "@hegel/typings": "^0.0.45",
    "flow-remove-types": "^2.173.0"
  },
  "type": "module"
}
